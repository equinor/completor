CONTROL_CRITERIA
  FUNCTION: [UDQ]
  ASSIGN FUTOL 0.05 /
  ASSIGN FUWCTWEL 1.5 /
  DEFINE FUWELMIN FUWCTWEL - FUTOL /
  DEFINE FUWELMAX FUWCTWEL + FUTOL /
/


CONTROL_CRITERIA
  FUNCTION: [OPEN, OPEN_WAIT, OPEN_READY]
  CRITERIUM: 1
  ICV: [A][B][C][D][E][F]
  FUWCT_x0 < FUWCTWEL AND /
  FUPOS_x0 < 10 /
/

CONTROL_CRITERIA
  FUNCTION: [OPEN_STOP, OPEN_WAIT_STOP]
  CRITERIUM: 1
  ICV: [A][B][C][D][E][F]
  FUWCT_x0 > FUWELMIN
/

CONTROL_CRITERIA
  FUNCTION: [CHOKE, CHOKE_WAIT, CHOKE_READY]
  CRITERIUM: 1
  ICV: [A][B][C][D][E][F]
  FUWCT_x0 > FUWCTWEL AND /
  FUPOS_x0 > 1 /
/

CONTROL_CRITERIA
  FUNCTION: [CHOKE_STOP, CHOKE_WAIT_STOP]
  CRITERIUM: 1
  ICV: [A]
  FUWCT_x0 < FUWELMAX
/

